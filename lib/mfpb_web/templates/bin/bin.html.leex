<div id="requests-container" phx-update="prepend">
  <%= for r <- @requests do %>
    <div id="<%= r.id %>">
      <h4>ID: <a href="#<%= r.id %>"><%= r.id %></a></h4>
      <h5>Received at: <%= DateTime.to_iso8601(r.timestamp) %></h5>
      <%= render("request.html", request: r) %>
      <hr>
    </div>
  <% end %>
</div>
